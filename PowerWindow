# MockProject

void setup()
{
  pinMode(13, OUTPUT);
  pinMode(12, OUTPUT);
  pinMode(11, INPUT);
 
}
void loop()
{
  if(digitalRead(12)==HIGH){
     digitalWrite(10,HIGH);
 if(digitalRead(11)==HIGH)
    {
      digitalWrite(13,HIGH);
  	}
  else
      analogWrite(13,0);
  }
  else  
  {
    digitalWrite(10,LOW);
    analogWrite(13,0);
  }
}

    

 
  

